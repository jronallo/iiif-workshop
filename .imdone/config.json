{
  "exclude": [
    "^(node_modules|bower_components|\\.imdone|target|build|dist|logs)[\\/\\\\]?|\\.(git|svn|hg|npmignore)|\\~$|\\.(jpg|png|gif|swp|ttf|otf)$"
  ],
  "watcher": true,
  "keepEmptyPriority": false,
  "code": {
    "include_lists": [
      "backlog",
      "todo",
      "next",
      "doing"
    ]
  },
  "lists": [
    {
      "name": "backlog",
      "hidden": false
    },
    {
      "name": "todo",
      "hidden": false
    },
    {
      "name": "next",
      "hidden": false
    },
    {
      "name": "doing",
      "hidden": false
    }
  ],
  "marked": {
    "gfm": true,
    "tables": true,
    "breaks": false,
    "pedantic": false,
    "smartLists": true,
    "langPrefix": "language-"
  }
}
